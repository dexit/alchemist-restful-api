{
    "name": "nvmcommunity/alchemist-restful-api",
    "description": "A library that helps you quickly get a rigorous and flexible RESTful-based API interface for your application.",
    "keywords": [
        "restful api",
        "api interface",
        "restful-based api interface",
        "alchemist restful api",
        "nvmcommunity",
        "nvmcommunity.io",
        "Người Viết Mã"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nvmcommunity\\Alchemist\\RestfulApi\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/nvmcommunity/alchemist-restful-api/issues",
        "source": "https://github.com/nvmcommunity/alchemist-restful-api"
    },
    "authors": [
        {
            "name": "NVM Community (Người Viết Mã)",
            "email": "nvmcommunity.io@gmail.com",
            "homepage": "https://nvmcommunity.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "nette/utils": "^3.2",
        "psr/http-message": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.2",
        "phpunit/phpunit": "9.6.15"
    }
}
